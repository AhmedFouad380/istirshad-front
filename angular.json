{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "gadeer": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "server": {
  "builder": "@angular-devkit/build-angular:server",
  "options": {
    "outputPath": "dist/gadeer-server",
    "main": "src/main.server.ts",
    "tsConfig": "tsconfig.server.json"
  },
  "configurations": {
    "production": {
      "outputHashing": "media",
      "sourceMap": false,
      "optimization": true
    }
  }
},
"serve-ssr": {
  "builder": "@nguniversal/builders:ssr-dev-server",
  "options": {
    "browserTarget": "gadeer:build",
    "serverTarget": "gadeer:server"
  },
  "configurations": {
    "production": {
      "browserTarget": "gadeer:build:production",
      "serverTarget": "gadeer:server:production"
    }
  }
},
"prerender": {
  "builder": "@nguniversal/builders:prerender",
  "options": {
    "browserTarget": "gadeer:build:production",
    "serverTarget": "gadeer:server:production",
    "routes": ["/"]
  }
}
,
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/gadeer",
            "index": "src/index.html",
            "browser": "src/main.ts",
            "polyfills": ["zone.js"],
            "tsConfig": "tsconfig.app.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "public",
                "output": "/public"
              },
              {
                "glob": "**/*",
                "input": "src/assets/images",
                "output": "/assets/images"
              }
              ,
              "src/firebase-messaging-sw.js"

            ],
            "allowedCommonJsDependencies": [
  "@zegocloud/zego-uikit-prebuilt",
  "pusher-js"
],
            "styles": [
              "node_modules/ngx-toastr/toastr.css",
              "node_modules/swiper/swiper-bundle.min.css"
              ,
            "src/styles.css"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2.5MB",
                  "maximumError": "6.5MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "4kB",
                  "maximumError": "8kB"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "gadeer:build:production"
            },
            "development": {
              "buildTarget": "gadeer:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "tsconfig.spec.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "public"
              }
            ],
            "styles": [
              "src/styles.css"
            ],
            "scripts": []
          }
        }
      }
    }
  },
  "cli": {
    "analytics": "d735b7a8-71bb-4879-a0af-4c8b33e92b75"
  }
}
